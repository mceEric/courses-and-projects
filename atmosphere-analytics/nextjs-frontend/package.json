{
  "name": "nextjs-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "cypress": "cypress open",
    "test": "start-server-and-test start http://localhost:3000 e2e:chrome",
    "e2e:chrome": "cypress run --browser chrome",
    "e2e:parallel": "cypress run --parallel",
    "vapid": "web-push generate-vapid-keys",
    "www": "cypress run && nyc report --reporter=text --reporter=text-summary",
    "www2": "nyc instrument --compact false components instrumented"
  },
  "dependencies": {
    "@babel/core": "^7.20.12",
    "@emotion/react": "^11.10.8",
    "@emotion/styled": "^11.10.8",
    "@headlessui/react": "^1.7.7",
    "@heroicons/react": "^2.0.13",
    "@influxdata/influxdb-client": "^1.33.1",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.2",
    "@mui/x-data-grid": "^6.3.0",
    "@nivo/bar": "^0.80.0",
    "@nivo/core": "^0.80.0",
    "@nivo/geo": "^0.80.0",
    "@nivo/line": "^0.80.0",
    "@nivo/pie": "^0.80.0",
    "@react-google-maps/api": "^2.18.1",
    "@tailwindcss/forms": "^0.5.3",
    "axios": "^1.2.1",
    "babel-plugin-istanbul": "^6.1.1",
    "cypress-browser-permissions": "^1.1.0",
    "daisyui": "^2.51.5",
    "formik": "^2.2.9",
    "next": "12.3.1",
    "next-pwa": "^5.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-loader-spinner": "^5.3.4",
    "react-onesignal": "^2.0.4",
    "web-push": "^3.5.0",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@cypress/code-coverage": "^3.10.4",
    "@netlify/plugin-nextjs": "^4.29.4",
    "@nivo/bar": "^0.80.0",
    "@nivo/core": "^0.80.0",
    "@nivo/line": "^0.80.0",
    "autoprefixer": "^10.4.12",
    "cypress": "^10.10.0",
    "eslint": "8.25.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "^8.6.0",
    "postcss": "^8.4.18",
    "cypress-localstorage-commands": "^2.2.2",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.2.0"
  },
  "nyc": {
    "exclude": [
      "components/**/Settings.jsx",
      "services/*.js",
      "components/**/QuestionnaireSubmissions.jsx"
    ],
    "excludeAfterRemap": true
  }
